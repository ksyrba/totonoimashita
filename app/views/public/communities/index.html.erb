<div class="container">
  <div class="row title">
    <h3>コミュニティ一覧</h3>
    <span>（全<%= @communities.count %>件）</span>
  </div>
  <div class="row d-flex justify-content-end">
    <div class="search">
      <%= render 'search', community: @community %>
    </div>
  </div>
  <div class="row">
    <div class="community_index col-12">
      <%= render 'index', communities: @communities %>
    </div>
  </div>
</div>
