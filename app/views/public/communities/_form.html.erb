<%= form_with model:community, local:true do |f| %>
  <div class="form-group">
    <%= f.label :"施設名" %>
    <%= f.text_field :community_name, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :"所在地" %>
    <%= f.collection_select(:area, Area.all, :id, :name, {}, {class:"area-select"}) %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>